entrypoint = "main.go"
run = ["go", "run", "."]

modules = ["go-1.21"]

[nix]
channel = "stable-23_11"

[gitHubImport]
requiredFiles = [".replit"]

[deployment]
ignorePorts = false
deploymentTarget = "static"
publicDir = "http"

[[ports]]
localPort = 8080
externalPort = 80

[env]
REPLIT_DISABLE_PACKAGE_LAYER = "1"
